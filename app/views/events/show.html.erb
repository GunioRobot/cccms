<% content_for :subnavigation do %>
  <%= link_to 'back', edit_node_path(@event.node) %>
  <%= link_to 'edit', edit_event_path(@event) %>
<% end %>

<h2>Event for node <%= @event.node.unique_name %></h2>

<p>
  <b>Start time:</b>
  <%=h @event.start_time %>
</p>

<p>
  <b>End time:</b>
  <%=h @event.end_time %>
</p>

<p>
  <b>Rrule:</b>
  <%=h @event.rrule %>
</p>

<p>
  <b>Custom rrule:</b>
  <%=h @event.custom_rrule %>
</p>

<p>
  <b>Allday:</b>
  <%=h @event.allday %>
</p>

<p>
  <b>Url:</b>
  <%=h @event.url %>
</p>

<p>
  <b>Latitude:</b>
  <%=h @event.latitude %>
</p>

<p>
  <b>Longitude:</b>
  <%=h @event.longitude %>
</p>